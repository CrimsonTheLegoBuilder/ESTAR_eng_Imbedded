#include "fnd_controller.h"


void send(uint_8 X) {
	for (int i = 8; i >= 1; i--) {
		if (X & 0x80) {
			digitalWrite(_DIO, HIGH);
			HAL_GPIO_WritePin(FND_DIO_GPIO_Port, FND_DIO_Pin, HIGH);
		}
		else {
			digitalWrite(_DIO, LOW);
		}
		X <<= 1;
		digitalWrite(_SCLK, LOW);
		digitalWrite(_SCLK, HIGH);
	}
}
